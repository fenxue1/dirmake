{
  "version": 4,
  "configurations": [
    {
      "name": "Win64-MSVC",
      "intelliSenseMode": "windows-msvc-x64",
      "compilerPath": "cl.exe",
      "cppStandard": "c++17",
      "cStandard": "c11",
      "compileCommands": "${workspaceFolder}/../build/DirModeEx/compile_commands.json",
      "defines": ["UNICODE", "_UNICODE"],
      "includePath": [
        "${workspaceFolder}",
        "${workspaceFolder}/../include",
        "${env:QTDIR}/include",
        "${env:QTDIR}/include/QtCore",
        "${env:QTDIR}/include/QtGui",
        "${env:QTDIR}/include/QtWidgets"
      ],
      "browse": {
        "path": ["${workspaceFolder}"]
      }
    }
  ]
}